mixin mnu()
	-
		var nav= { 
			"Раздел 2":"#", 
			"Раздел 3":"#", 
			"Раздел 4":"#", 
			"Раздел 5":"#", 
			"Раздел 6":"#", 
			"Раздел 7":"#", 
			"Раздел 8":"#", 
			"Ежеквартальный отчет":"#", 
			}
	.nav-slider.swiper-container
		+b.UL.navMenu.swiper-wrapper(itemscope itemtype="http://schema.org/SiteNavigationElement")
			+e.LI.item.swiper-slide( itemprop="item" ): +e.A.link.active(href="#"  itemprop="url") Раздел 1
			each linkPatch, linkName in nav
				+e.LI.item.swiper-slide( itemprop="item" ): +e.A.link(href= linkPatch  itemprop="url")= linkName
//- /menu